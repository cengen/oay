                                <span class="fa fa-reorder"></span>
                            </span>
                        <span class="nb_right pull-right">Meny</span>
                    </a>

                </div>
                <div class="collapse navbar-collapse">
                    <ul class="nav pull-right navbar-nav">
                        <li>@Html.ActionLink("Hjem", "Index", "Home")</li>
                        <li class="dropdown">
                            <a data-toggle="dropdown" class="dropdown-toggle" href="#">Båtturer<span class="caret"></span></a>
                            <ul class="dropdown-menu">
                                @Html.Action("_MenyPartial", "Home")
                                <li>@Html.ActionLink("Våre turforslag", "Batturer", "Batturer")</li>
                            </ul>
                        </li>
                        <li>@Html.ActionLink("Meny", "Meny", "Meny")</li>
                        <li>@Html.ActionLink("Pris", "PrisInfo", "Pris")</li>
                        <li>@Html.ActionLink("Båten", "Bildegalleri", "Bildegalleri")</li>
                        @*<li class="dropdown">
                            <a data-toggle="dropdown" class="dropdown-toggle" href="#">Partnere<span class="caret"></span></a>
                            <ul class="dropdown-menu">
                                @Html.Action("_MenyPartnerePartial", "Samarbeidspartnere")
                                <li>@Html.ActionLink("Våre partnere", "Samarbeidspartnere", "Samarbeidspartnere")</li>
                            </ul>
                        </li>*@
                        <li>@Html.ActionLink("Kontakt oss", "KontaktOss", "Home")</li>
                    </ul>
                </div>
            </div>
        </div>
    </div>
</header>
<div id="body">
    @RenderSection("featured", required: false)
    <section class="content-wrapper main-content clear-fix">
        @RenderBody()
